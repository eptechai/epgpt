    "flowbite-svelte": "^0.44.15",
    "flowbite-svelte-icons": "^0.4.4",
    "node-sass": "^9.0.0",
    "openapi-typescript-codegen": "^0.25.0",
    "sass": "^1.64.2",
    "svelte-autosize": "^1.1.0",
    "svelte-collapsible": "^0.2.2",
    "svelte-file-dropzone": "^2.0.2",
    "tailwind-scrollbar": "^3.0.5",
    "uuid": "^9.0.1"
